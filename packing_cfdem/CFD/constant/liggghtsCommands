FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      liggghtsCommands;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// LIGGGHTS命令模型配置
// 用于控制CFDEM与LIGGGHTS之间的交互
liggghtsCommandModels
(
    runLiggghts      // 运行LIGGGHTS求解器
    writeLiggghts    // 输出LIGGGHTS数据
);

// 输出控制参数
writeLiggghtsProps
{
    writeLast       off;        // 不只输出最后时间步
    overwrite       on;         // 覆盖已有文件
    verbose         true;       // 详细输出信息
}

// ************************************************************************* //